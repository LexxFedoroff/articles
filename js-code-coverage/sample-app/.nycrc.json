{
    "all": true,
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx"
    ],
    "exclude": [
        "cypress/**/*.*",
        "**/*.d.ts",
        "**/*.cy.tsx",
        "**/*.cy.ts",
        "**/*.test.ts",
        "src/main.tsx"
    ],
    "reporter": [
        "json"
    ],
    "report-dir": "./.coverage-data/cypress"
}